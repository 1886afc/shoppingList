{
  "name": "shoppinglist",
  "version": "1.0.0",
  "description": "shopping list desktop app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ShoppingList\"",
    "package-linux" : "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },

  "repository": {
    "type": "git",
    "url": "git+https://github.com/1886afc/shoppingList.git"
  },
  "author": "jacob",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1886afc/shoppingList/issues"
  },
  "homepage": "https://github.com/1886afc/shoppingList#readme",
  "dependencies": {
    "electron": "^1.7.6",
    "path": "^0.12.7",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "electron-package": "^0.1.0"
  }
}
